{
  "name": "Day Percent",
  "description": "Simple Day Percent for better schedule your life. You can set the start time and end time.",
  "version": "1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "bg-wrapper.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}